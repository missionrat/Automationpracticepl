<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Test Suite" parallel="methods">
	<test thread-count="5" name="UI Automation Test">
		<parameter name="browser" value="${browser}" />
		<parameter name="isLambdaTest" value="${isLambaTest}" />
		<parameter name="isHeadless" value="${isHeadless}" />
		<classes>
			<class name="com.ui.tests.LoginTestNew" />
			<class name="com.ui.tests.InvalidCredentialsLoginTest" />
			<class name="com.ui.tests.SearchProductTest" />
			<class name="com.ui.tests.AddNewAddressTest" />
			<class name="com.ui.tests.ProductCheckoutTest" />

		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->


